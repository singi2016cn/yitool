{
    "name": "singi2016cn/yitool",
    "description": "tools for php, Inspired by hutool",
    "license": "MIT",
    "authors": [
        {
            "name": "singi2016cn",
            "email": "787575327@qq.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-xml": "*"
    },
    "type": "library",
    "keywords": [
        "yitool"
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": ["tests/HelloWorldTest.php"]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "files": ["tests/HelloWorldTest.php"]
    }
}

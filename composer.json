{
  "name": "singi2016cn/yitool",
  "description": "tools for php, Inspired by looly/hutool",
  "license": "MIT",
  "authors": [
    {
      "name": "singi2016cn",
      "email": "787575327@qq.com"
    }
  ],
  "config": {
    "platform": {
      "php": "7.3"
    },
    "sort-packages": true
  },
  "prefer-stable": true,
  "require": {
    "php": ">=7.3",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-xml": "*"
  },
  "type": "library",
  "keywords": [
    "yitool"
  ],
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "YiTool\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YiTool\\Tests\\": "tests/"
    },
    "classmap": [
      "tests/",
      "tests/HelloWorldTest.php"
    ]
  }
}
